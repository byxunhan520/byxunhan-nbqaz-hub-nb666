local ui = loadstring(game:HttpGet("https://raw.githubusercontent.com/odhdshhe/lenglengui/refs/heads/main/JMJMUI.lua"))()        
local win = ui:new("Ëä±ËêΩ<ÂØªÊ∂µ>")
local UITab84 = win:Tab("ÂäüËÉΩ",'16060333448')

local about = UITab84:section("„ÄéÂØª„Äè",false)
local tspeed = 16
local tpwalking = false
local player = game:GetService("Players")
local lplr = player.LocalPlayer

about:Label("ÊàëÊòØÁºùÂêàÂ∞èÂ≠ê")
about:Label("‰∏çË¶ÅÁ¨ëÊàë")
about:Label("Â∑≤ÁªèËÄÅÂÆû")
about:Label("ü§´‚úäüèøüòé")
-- ÁßªÂä®ÈÄüÂ∫¶ÊªëÊù°
about:Slider("ÁßªÂä®ÈÄüÂ∫¶", "Ë∞ÉÊï¥ËßíËâ≤ÁßªÂä®ÈÄüÂ∫¶", 1, 1, 250, false, function(value)
    tspeed = value
end)

-- ‰º†ÈÄÅÁßªÂä®ÂºÄÂÖ≥
about:Toggle("‰º†ÈÄÅÁßªÂä®", "ÂêØÁî®/Á¶ÅÁî®‰º†ÈÄÅÁßªÂä®", false, function(state)
    tpwalking = state
end)

-- ‰º†ÈÄÅÁßªÂä®ÈÄªËæë
task.spawn(function()
    while true do
        if tpwalking then
            local chr = lplr.Character
            local hum = chr and chr:FindFirstChildWhichIsA("Humanoid")
            local hrp = chr and chr:FindFirstChild("HumanoidRootPart")

            if hum and hrp and hum.MoveDirection.Magnitude > 0 then
                local speedFactor = tspeed / 16
                local moveVector = hum.MoveDirection * (speedFactor * 2)
                hrp.CFrame = hrp.CFrame + moveVector
            end
        end
        task.wait(0) 
    end
end)

local JumpPowerValue = 100
about:Slider("Ë∑≥", "Ë∑≥È´ò", 1, 75, 250, false, function(value)
    JumpPowerValue = value
end)
about:Toggle("Ë∑≥", "ÂêØÁî®/Á¶ÅÁî®", false, function(Value)
    if Value then
        game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = JumpPowerValue
    else
        game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = 50
    end
end)
